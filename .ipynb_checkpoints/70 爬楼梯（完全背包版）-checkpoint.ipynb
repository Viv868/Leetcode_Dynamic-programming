   "source": [
    "def climbStairs(n):\n",
    "    stair = [1,2]\n",
    "    dp = [0] * (n+1)\n",
    "    dp[0] = 1\n",
    "    \n",
    "    for j in range(n+1):\n",
    "        for s in stair:\n",
    "            if j >= s:\n",
    "                dp[j] += dp[j-s]\n",
    "    return dp[-1]\n",
    "\n",
    "climbStairs(3)"
   ]
  }
