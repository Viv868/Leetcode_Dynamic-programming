   "source": [
    "\n",
    "def numTrees(n):\n",
    "    dp = [0] * (n+1)\n",
    "    dp[0] = 1 \n",
    "    \n",
    "    for i in range(1, n+1):\n",
    "        for j in range(i):\n",
    "            dp[i] += dp[j] * dp[i-j-1]\n",
    "    return dp[-1]\n",
    "numTrees(4)"
   ]
  }
